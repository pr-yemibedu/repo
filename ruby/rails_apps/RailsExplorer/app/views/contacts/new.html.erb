<h1>New contact</h1>

<%= error_messages_for :contact %>

<% form_for(@contact) do |f| %>
  <p>
    <b>First</b><br />
    <%= f.text_field :first %>
  </p>

  <p>
    <b>Last</b><br />
    <%= f.text_field :last %>
  </p>

  <p>
    <b>Age</b><br />
    <%= f.text_field :age %>
  </p>

  <p>
    <b>Birthday</b><br />
    <%= f.date_select :birthday %>
  </p>

  <p>
    <b>Notes</b><br />
    <%= f.text_area :notes %>
  </p>

  <p>
    <b>Married</b><br />
    <%= f.check_box :married %>
  </p>

  <p>
    <b>Version</b><br />
    <%= f.text_field :version %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', contacts_path %>
